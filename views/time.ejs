</head>
<body>
    <p> I am from time.ejs</p>
    <% include includes/bill.ejs %>
    <form method="post" action="/service/time/address">
        <p name="etime" id="etime">Earliest available time: <%= oavailabletime[0] %></p>
        <script>
            var date = new Date();
        </script>
        <button type="button" onclick="earliestTime(this.value)" value="<%=oavailabletime[0]%>">Select Earliest Time</button>
        <select name="servicedate" id="servicedate"></select>
        <select name="servicetime" id="servicetime">
            <% for(var value in oavailabletime){ %>
                <option value=<%- oavailabletime[value] %>><%- JSON.stringify(oavailabletime[value]) %></option>
            <% } %> 
        </select>
        <input type="submit" value="submit">
    </form>
    <script src="/scripts/time.js"></script>
</body>
</html>